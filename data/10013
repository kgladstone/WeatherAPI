<script type="text/template" id="table-horizontal-details-template">
	<% for (var index = 0; index < weather.forecast.days.length; index++) { %>
		<%
			var day = weather.forecast.days[index];
			var astronomyDay = weather.astronomy && weather.astronomy.days.length ? weather.astronomy.days[index] : null;
			var latitude = weather.response && weather.response.location && typeof weather.response.location.latitude !== 'undefined' ? weather.response.location.latitude : null;
			var moonDirection = 'moonNorth';
			var errorString = 'Not Available';
		%>
		<% if (day.summary) { %>
		<div id="horizontal-day-<%= day.summary.date.yday %>" class="fctDetails">
		<div class="fctText row">
		<% _.each([day.summary.day, day.summary.night], function(period, index) { %>
		<% if (period.title !== null) { %>
		<%
								var snowRange = period.snow_range;
								var showUnits = true;
								var isSnow = period.precip_type === 'snow';

								if (snowRange == null) {
		                             if (weather.forecast.source === 'nws') {
		                                snowRange = '--';
		                                showUnits = false;
		                             } else {
		                                snowRange = weather.response.units === 'metric' ? '< 2' : '< 1';
		                             }
		                        }

								var precipAmount = isSnow ? snowRange : (period.liquid_precip || 0);
								var precipUnits = isSnow ? labels.snow_depth.units : labels.rainfall.units;
							%>
		<div class="small-12 medium-6 large-6 columns">
		<div class="fct-icon">
		<img src="<%= period.icon_url %>" class="no-scale" />
		</div>
		<div class="fct-content">
		<span class="fct-title"><%= period.title %></span>
		<span class="fct-date"><%= wui.date.strftime("%m/%d",day.summary.date) %></span>
		<span class="precip-wrap">
		<span class="pop<% if (period.precip_type === 'snow') {%> snow<% } if(period.pop >= 30) {%> wet<%}%>" title="<%= labels.pop.label %>"></span><strong><%= period.pop %></strong><%= labels.pop.units %> / <strong><%= precipAmount %></strong> <% if (showUnits || !isSnow) { %><%= precipUnits %><% } %>
		</span>
		<p><%= period.text %></p>
		</div>
		</div>
		<% } %>
		<% }); %>
