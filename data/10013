	<h3>History</h3>
	<table id="almanac-history">
		<!-- insert almanac history template -->
	</table>
	<div class="footer">
		<a class="left" href="/history/airport/KJRB/2015/6/11/MonthlyCalendar.html?req_city=New York&req_state=NY&reqdb.zip=10013&reqdb.magic=1&reqdb.wmo=99999#calendar"><i class="fi-calendar"></i> June Calendar View</a>
