<script type="text/template" id="extended-template">
	<div id="extented-forecat">
		<% _.each(weather.forecast.days, function(period,periodindex) { %>
		<% if (period.summary.day.text !== null) { %>
		<%
					var snowRangeDay = period.summary.day.snow_range;
					var showUnitsDay = true;
					var isSnowDay = period.summary.day.precip_type === 'snow';

                    if (snowRangeDay == null) {
                         if (weather.forecast.source === 'nws') {
                            snowRangeDay = '--';
                            showUnitsDay = false;
                         } else {
                            snowRangeDay = weather.response.units === 'metric' ? '< 2' : '< 1';
                         }
                    }

					var precipAmountDay = isSnowDay ? snowRangeDay : (period.summary.day.liquid_precip || 0);
					precipUnitsDay = isSnowDay ? labels.snow_depth.units : labels.rainfall.units;

					var snowRangeNight = period.summary.night.snow_range;
					var showUnitsNight = true;
					var isSnowNight = period.summary.night.precip_type === 'snow';

                    if (snowRangeNight == null) {
                         if (weather.forecast.source === 'nws') {
                            snowRangeNight = '--';
                            showUnitsNight = false;
                         } else {
                            snowRangeNight = weather.response.units === 'metric' ? '< 2' : '< 1';
                         }
                    }

					var precipAmountNight = isSnowNight ? snowRangeNight : (period.summary.night.liquid_precip || 0);
					precipUnitsNight = isSnowNight ? labels.snow_depth.units : labels.rainfall.units;
				%>
		<div id="fctDay-<%= period.summary.date.year %><%= wui.date.pad(period.summary.date.month) %><%= wui.date.pad(period.summary.date.day) %>" class="day-row <% if (periodindex>=3) { %>fct3<% } %>">
		<div class="summary">
		<span class="weekday"><%= period.summary.date.weekday %></span>
		<span class="date"><%= wui.date.strftime("%m/%d",period.summary.date) %></span>
		<span class="temps">
		<span class="high"><%= period.summary.high %></span>
		<span class="split">|</span>
		<span class="low"><%= period.summary.low %></span>
		<%= labels.temperature.units %>
		</span>
		</div>
		<div class="day">
		<img src="<%= period.summary.day.icon_url %>" />
		<span class="title"><span class="long"><%= period.summary.date.weekday %></span><span class="short"><%= period.summary.date.weekday_short %></span></span>
		<span class="pop<% if (period.summary.day.precip_type === 'snow') {%> snow<% } if(period.summary.day.pop >= 30) {%> wet<%}%>" title="<%= labels.pop.label %>"><span><%= period.summary.day.pop %></span><%= labels.pop.units %> <%= labels.precipitation.abbrev %> / <strong><%= precipAmountDay %></strong> <% if (showUnitsDay || !isSnowDay) { %><%= precipUnitsDay %><% } %></span>
		<p><%= period.summary.day.text %></p>
		</div>
		<div class="night">
		<img src="<%= period.summary.night.icon_url %>" />
		<span class="title"><span class="long"><%= period.summary.date.weekday %></span><span class="short"><%= period.summary.date.weekday_short %></span> Night</span>
		<span class="pop<% if (period.summary.night.precip_type === 'snow') {%> snow<% } if(period.summary.night.pop >= 30) {%> wet<%}%>" title="<%= labels.pop.label %>"><span><%= period.summary.night.pop %></span><%= labels.pop.units %> <%= labels.precipitation.abbrev %> / <strong><%= precipAmountNight %></strong> <% if (showUnitsNight || !isSnowNight) { %><%= precipUnitsNight %><% } %></span>
		<p><%= period.summary.night.text %></p>
		</div>
		</div>
		<% } %>
		<% }); %>
	</div>
	<p class="fct3-toggle">
		<a href="#" class="fct3-toggle-show">More Days</a>
		<a href="#" class="fct3-toggle-hide">Fewer Days</a>
