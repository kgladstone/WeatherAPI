<script type="text/template" id="stationselector_table_template">
	<tbody>
	<% _.each(stations, function(station) { %>
		<%
			var isPWS = (station.type == 'PWS');
			var query = (isPWS ? 'pws:' : '') + station.id;
			var isExternal = (radius && station._distance > radius);
		var classNames = [];
		classNames.push(isPWS ? 'is-pws' : 'is-airport');
		if (isExternal) { classNames.push('ext-link'); }
		var classNamesStr = classNames.join(' ');
		var title = station._title + ' (' + station.id + ')';
		%>
		<tr class="<%= classNamesStr %>">
		<td><span class="station-icon"></span></td>
		<td><a class="stationselectorRowPWS" data-station="<%= station.id %>" href="/cgi-bin/findweather/getForecast?query=<%= query %>" title=""><%= title %></a></td>
		<td><span class="temp"><%= station.temperature %></span>&deg;</td>
		</tr>
	<% }); %>
	</tbody>
</script>
