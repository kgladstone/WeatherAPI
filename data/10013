<script type="text/template" id="almanac-history-template">
	<% var forecastToday=weather.forecast.days[0].summary %>
	<% var planner=history.trip %>
	<% var almanac=history.almanac %>
	<% var yesterday=yesterday %>
	<% var unitType= wui.bootstrapped.prefs.Units %>
	<% var tempUnit= (unitType=="english") ? "&deg;F" : "&deg;C" %>
	<% var fallUnit= (unitType=="english") ? "in" : "mm" %>
	<% var speedUnit= (unitType=="english") ? "mph" : "kph" %>
	<% 
		var temp = {
				forecast: {},
				avg: {},
				range: {
					high: {},
					low: {} 
				}
			}; 
		var precip = {
				forecast: {},
				avg: {},
				range: {
					rain: {}
				}
			};
		var dew = {
				avg: {},
				range: {
					low: {},
					high: {}
				}
			};
		var record = {
				high: {
					record: "",	
					year: ""	
				},
				low: {
					record: "",	
					year: ""	
				}
			};
		var yesterday_data = {
				temp: {
					high: "",
					low: ""
				},
				heating_degree_days: ""
			};
	
	//V2 Forecast data
	temp.forecast.high = forecastToday.high;
	temp.forecast.low = forecastToday.low;

	precip.forecast.rain = forecastToday.liquid_precip;
	precip.forecast.snow = forecastToday.snow;

	record.high.year = almanac.temp_high.recordyear;
	record.low.year = almanac.temp_low.recordyear;

		if(unitType == "english") {

			temp.avg.high 		= almanac.temp_high.normal.F;
			temp.avg.low 		= almanac.temp_low.normal.F;
			temp.range.high.min = planner.temp_high.min.F;
			temp.range.high.max = planner.temp_high.max.F;
			temp.range.low.min 	= planner.temp_low.min.F;
			temp.range.low.max 	= planner.temp_low.max.F;

			precip.avg.rain 		= planner.precip.avg.in;
			precip.range.rain.min 	= planner.precip.min.in;
			precip.range.rain.max 	= planner.precip.max.in;

			dew.avg.low 		= planner.dewpoint_low.avg.F;
			dew.avg.high 		= planner.dewpoint_high.avg.F;
			dew.range.low.min 	= planner.dewpoint_low.min.F;
			dew.range.low.max 	= planner.dewpoint_low.max.F;
			dew.range.high.min 	= planner.dewpoint_high.min.F;
			dew.range.high.max 	= planner.dewpoint_high.max.F;

			record.high.record 	= almanac.temp_high.record.F;
			record.low.record 	= almanac.temp_low.record.F;

			yesterday_data.temp.high = yesterday.history.dailysummary[0].maxtempi;
			yesterday_data.temp.low = yesterday.history.dailysummary[0].mintempi;
			yesterday_data.heating_degree_days = yesterday.history.dailysummary[0].heatingdegreedays;
			
		} else {

			temp.avg.high 		= almanac.temp_high.normal.C;
			temp.avg.low 		= almanac.temp_low.normal.C;
			temp.range.high.min = planner.temp_high.min.C;
			temp.range.high.max = planner.temp_high.max.C;
			temp.range.low.min 	= planner.temp_low.min.C;
			temp.range.low.max 	= planner.temp_low.max.C;

			precip.avg.rain 		= planner.precip.avg.cm;
			precip.range.rain.min 	= planner.precip.min.cm;
			precip.range.rain.max 	= planner.precip.max.cm;

			dew.avg.low 		= planner.dewpoint_low.avg.C;
			dew.avg.high 		= planner.dewpoint_high.avg.C;
			dew.range.low.min 	= planner.dewpoint_low.min.C;
			dew.range.low.max 	= planner.dewpoint_low.max.C;
			dew.range.high.min 	= planner.dewpoint_high.min.C;
			dew.range.high.max 	= planner.dewpoint_high.max.C;

			record.high.record 	= almanac.temp_high.record.C;
			record.low.record 	= almanac.temp_low.record.C;
			
			yesterday_data.temp.high = yesterday.history.dailysummary[0].maxtempm;
			yesterday_data.temp.low = yesterday.history.dailysummary[0].mintempm;
			yesterday_data.heating_degree_days = yesterday.history.dailysummary[0].heatingdegreedays;
		}	
	%>
